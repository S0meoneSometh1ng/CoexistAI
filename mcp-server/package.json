{
  "name": "coexistai-mcp",
  "version": "1.0.0",
  "description": "Production-ready MCP server for CoexistAI - AI research assistant with web search, YouTube, Reddit, maps, and more",
  "type": "module",
  "main": "./build/index.js",
  "bin": {
    "coexistai-mcp": "./build/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x build/index.js",
    "dev": "tsx watch src/index.ts",
    "start": "node build/index.js",
    "prepare": "npm run build",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "research",
    "web-search",
    "youtube",
    "reddit",
    "maps",
    "github"
  ],
  "author": "CoexistAI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SPThole/CoexistAI.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.6.7",
    "zod": "^3.22.4",
    "dotenv": "^16.4.5",
    "winston": "^3.11.0",
    "execa": "^8.0.1",
    "p-retry": "^6.2.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "typescript": "^5.3.3",
    "tsx": "^4.7.1",
    "vitest": "^1.3.1",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
